     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler2D diffuseMap;
sampler2D bumpMap;
float4 specularColor;

struct PS_INPUT
{
   float2 TexCoords       : TEXCOORD0;
   float3 Normal          : TEXCOORD1;
   float3 View            : TEXCOORD2;
   float3 LightDir        : TEXCOORD3;
};



float4 ps_main(PS_INPUT Input) : COLOR0
{   
   Input.Normal      = normalize(Input.Normal);
   Input.View        = normalize(Input.View);
   Input.LightDir    = normalize(Input.LightDir);
   
   float4 color      = {0.0f,1.0f,0.0f,1.0f};
   //float4 color = tex2D(diffuseMap, Input.TexCoords);
   float4 bump       = tex2D(bumpMap,Input.TexCoords);
   
   float offsetNormal = bump.z - 0.5f;
   Input.Normal       += Input.Normal*offsetNormal * 0.1;
   float diffuse      = dot(Input.Normal, Input.LightDir);
   
   
   float3 Ln          = dot(Input.Normal, Input.LightDir) * Input.Normal;
   float3 Lt          = Input.LightDir - Ln;
   float3 Rl          = Ln - Lt;
   float RdotV        = max(0.0f, dot(Input.View, Rl));
   float4 specular    = pow(RdotV , 25) * specularColor;
   
   return  saturate( color * diffuse + specular);
   
}



]]></RmShaderCode>
