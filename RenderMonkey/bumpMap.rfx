float4x4 matViewProjection;
float4x4 matView;
float4x4 matViewTranspose;
float4 vViewPosition;
float3 fvLightPosition;

struct VS_INPUT 
{
   float4 Position    : POSITION0;
   float2 TexCoords   : TEXCOORD0;
   float3 Normal      : NORMAL0;
};

struct VS_OUTPUT 
{
   float4 Position       : POSITION0;
   float2 TexCoords      : TEXCOORD0;
   float3 Normal         : TEXCOORD1;
   float3 View           : TEXCOORD2;
   float3 LightDir       : TEXCOORD3;
};

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   Output.Position     = mul( Input.Position, matViewProjection );
   Output.TexCoords    = Input.TexCoords;
   
   float3 lightPos     = mul( fvLightPosition, matView);
   
   float3 viewSpacePos = mul( Input.Position, matView );
   Output.Normal       = mul( Input.Normal, matView ) ;
   
   Output.View         = - viewSpacePos;
   Output.LightDir     = (lightPos - viewSpacePos);
   
   return( Output );
   
}



]]></RmShaderCode>
