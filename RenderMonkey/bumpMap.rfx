sampler2D sColorMap;
sampler2D sDepthMap;
sampler2D sNormalMap;

float4 fAmbientIntensity;
float4 fDiffuseIntensity;
float4 fSpecularIntensity;

struct PS_INPUT 
{
   float2 Tex      : TEXCOORD0;
   float3 ViewDir  : TEXCOORD1;
   float3 LightDir : TEXCOORD2;
};

float4 ps_main(PS_INPUT Input) : COLOR0
{   
   Input.LightDir = normalize( Input.LightDir );
   Input.ViewDir  = normalize( Input.ViewDir );
      
   float4 cColor = tex2D( sColorMap, Input.Tex );
   float3 vNormal = normalize( tex2D( sNormalMap, Input.Tex).xyz * 2.0f - 1.0f);
   float  fDepth = tex2D( sDepthMap, Input.Tex ).x;
   
   float LdotN  = max( 0, dot( vNormal, Input.LightDir ) );
   float3 Ln    = LdotN * vNormal;
   float3 Lt    = Input.LightDir - Ln;
   float3 R     = Input.LightDir - Lt;
   float RdotV  = max( 0, dot( R, Input.ViewDir ) );
   float4 cAmbient = fAmbientIntensity * cColor;
   float4 cDiffuse = fDiffuseIntensity * LdotN * cColor;
   float4 cSpecular = fSpecularIntensity * pow( RdotV, 25 );
   return saturate( cAmbient + cDiffuse + cSpecular);
}



]]></RmShaderCode>
