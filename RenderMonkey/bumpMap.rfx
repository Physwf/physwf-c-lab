     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matViewProjection;
float4x4 matView;
float3 fvLightPosition;
float4 vViewPosition;

struct VS_INPUT 
{
   float4 Position : POSITION0;
   float2 TexCoords: TEXCOORD0;
   float3 Normal   : NORMAL0;
   float3 Binormal : NORMAL1;
   float3 Tangent  : NORMAL2;
};

struct VS_OUTPUT 
{
   float4 Position : POSITION0;
   float2 Tex      : TEXCOORD0;
   float3 ViewDir  : TEXCOORD1;
   float3 LightDir : TEXCOORD2;
};

VS_OUTPUT vs_main( VS_INPUT Input )
{
   VS_OUTPUT Output;

   Output.Position = mul( Input.Position, matViewProjection );
   Output.Tex = Input.TexCoords;
   
   //view space
   float3 vPosition = mul( Input.Position, matView );
   float3 vNormal   = mul( Input.Normal, matView );
   float3 vBinormal = mul( Input.Binormal, matView );
   float3 vTrangent = mul( Input.Tangent, matView );
   
   float3 vLightPosition = mul( fvLightPosition, matView );
   float3 vLightDir      = mul( vLightPosition, matView );
   //tangent space
   Output.ViewDir.x = dot( -vPosition, vNormal );
   Output.ViewDir.y = dot( -vPosition, vBinormal );
   Output.ViewDir.z = dot( -vPosition, vTrangent );
   
   Output.LightDir.x = dot( vLightDir, vNormal );
   Output.LightDir.y = dot( vLightDir, vBinormal );
   Output.LightDir.z = dot( vLightDir, vTrangent );
   
   return( Output );
   
}



]]></RmShaderCode>
