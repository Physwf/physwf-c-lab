     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="0"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 fAmbientIntensity;
float4 fDiffuseIntensity;
float4 fSpecularIntensity;
float kAmbient;
float kDiffuse;
float kSpecular;

sampler2D sNormal;

struct PS_INPUT
{
      float2 Tex      : TEXCOORD0;
      float3 ViewDir  : TEXCOORD1;
      float3 LightDir : TEXCOORD2;
};

float4 ps_main(PS_INPUT Input) : COLOR0
{
   Input.LightDir   = normalize ( Input.LightDir );
   Input.ViewDir    = normalize ( Input.ViewDir );
   float3 vNormal   = tex2D( sNormal, Input.Tex ).xyz * 2 - 1.0f;
   vNormal          = normalize (vNormal);
   
   float4 cAmbient  = kAmbient * fAmbientIntensity;
   
   float  NdotL     = max( 0, dot( vNormal, Input.LightDir ));
   float4 cDiffuse  = kDiffuse * NdotL * fDiffuseIntensity;
   
   float3 Ln = dot( Input.LightDir, vNormal ) * vNormal;
   float3 Lt = Input.LightDir - Ln;
   float3 R = Ln - Lt;
   float  RdotV = max(0, dot( R, Input.ViewDir ));
   float4 cSpecular = kSpecular * pow( RdotV, 25 ) * fSpecularIntensity;
   return( saturate(cAmbient+cDiffuse+cSpecular) );
   
}



]]></RmShaderCode>
